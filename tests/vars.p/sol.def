def int f(int a, int b) {
    if (a == 3) {
        int a = 10 * b;
        while (a > 3) {
            int b = 2;
            a = a / b;
        }
    }
    else {
        int b = a * 3;
        while (b > a) {
            int a = 4;
            b = b / a;
        }
    }
    return a;
}

def int main() {
    print(f(7, 5));
}
